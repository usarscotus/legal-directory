---
layout: none
---

[
  {% assign docs = site.pages | where_exp: "p", "p.search_exclude != true and p.layout != 'page'" %}
  {% for doc in docs %}
    {
      "title": {{ doc.title | jsonify }},
      "url": "{{ site.baseurl }}{{ doc.url }}",
      "content": {{ doc.content | strip_html | normalize_whitespace | jsonify }},
      "breadcrumb_parent": {{ doc.breadcrumb_parent | default: "" | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
