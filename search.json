---
layout: none
---

[
  {% assign docs = site.pages | where_exp: "p", "p.search_exclude != true" %}
  {% for doc in docs %}
    {
      "title": {{ doc.title | jsonify }},
      "url": "{{ site.baseurl }}{{ doc.url }}",
      "content": {{ doc.content | strip_html | normalize_whitespace | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
