---
layout: null
permalink: /updates.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
<channel>
  <title>{{ site.title }} Updates</title>
  <link>{{ site.url }}{{ site.baseurl }}/updates.html</link>
  <description>Recent additions and changes to documents.</description>
  {% for item in site.data.updates %}
  <item>
    <title>{{ item.title }}</title>
    <description>{{ item.description }}</description>
    {% if item.url %}
    <link>{{ site.url }}{{ site.baseurl }}{{ item.url }}</link>
    <guid>{{ site.url }}{{ site.baseurl }}{{ item.url }}</guid>
    {% endif %}
    <pubDate>{{ item.date | date_to_rfc822 }}</pubDate>
  </item>
  {% endfor %}
</channel>
</rss>
